{
  "name": "nuxt-history-state",
  "version": "0.4.1",
  "description": "Nuxt.js module to backup or restore historical states",
  "main": "./lib/module.js",
  "types": "./types/module.d.ts",
  "files": [
    "lib",
    "types/*.d.ts"
  ],
  "scripts": {
    "test": "jest",
    "version.major": "standard-version --release-as major",
    "version.minor": "standard-version --release-as minor",
    "version.patch": "standard-version --release-as patch",
    "release": "git push --follow-tags origin master && npm publish"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hidekatsu-izuno/nuxt-history-state.git"
  },
  "author": "Hidekatsu Izuno <hidekatsu.izuno@gmail.com> (https://github.com/hidekatsu-izuno/)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/hidekatsu-izuno/nuxt-history-state/issues"
  },
  "homepage": "https://github.com/hidekatsu-izuno/nuxt-history-state#readme",
  "devDependencies": {
    "@nuxtjs/module-test-utils": "latest",
    "jest": "latest",
    "standard-version": "latest",
    "nuxt-edge": "latest"
  },
  "dependencies": {
    "css.escape": "^1.5.1",
    "lz-string": "^1.4.4"
  }
}
